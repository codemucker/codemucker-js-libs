{
    "name": "codemucker-codegen-ts-libs",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "main": "build/js/index.js",
    "types": "build/js/codemucker-codegen.d.ts",
    "scripts": {
        "prepublish": "npm run build",
        "clean-lite": "rm -fR build && rm -fR node_modules/.cache/*",
        "_run_cmd": "echo '' &&",
        "build": "tsc",
        "build-watch": "tsc --watch",
        "test_jest": "npm run build && npx jest",
        "test": "npm run build && node build/js/test/run-all.js",
        "test-watch": "watch 'npm run test' ./src/ ./test/",
        "generate": "node build/js/src/main.js",
        "generate-watch": "watch 'npm run build && npm run generate' ./src/",
        "docs": "npm run docs-better && npm run docs-tsdocs",
        "docs-better": "jsdoc -c jsdoc.json -r -d build/better-docs",
        "docs-tsdocs": "typedoc --out build/tsdocs src/ && typedoc --out build/tsdocs build/generated/main",
        "scratch": "tsc src/scratch.ts --outFile build/js/scratch.js && node build/js/scratch.js",
        "scratch-watch": "watch 'npm run scratch' ./src"
    },
    "dependencies": {
        "@log4js-node/log4js-api": "^1.0.2",
        "capture-stack-trace": "^1.0.1",
        "check-password-strength": "^1.0.8",
        "core-js": "^3.6.5",
        "eventemitter2": "^6.4.3",
        "execution-time": "^1.4.1",
        "file-system": "^2.2.2",
        "flatted": "^3.0.4",
        "moment": "^2.27.0",
        "quicktype-core": "^6.0.66",
        "uom-ts": "^1.1.0",
        "uuid": "^8.3.0",
        "watch": "^1.0.2"
    },
    "devDependencies": {
        "@babel/core": "^7.10.2",
        "@babel/preset-env": "^7.10.2",
        "@types/filesystem": "0.0.29",
        "@types/jest": "^26.0.0",
        "@types/lodash": "^4.14.155",
        "@types/node": "^14.0.13",
        "@types/uuid": "^8.0.1",
        "@typescript-eslint/eslint-plugin": "^2.33.0",
        "@typescript-eslint/parser": "^2.33.0",
        "@zeit/ncc": "^0.22.3",
        "babel-jest": "^26.0.1",
        "better-docs": "^2.0.1",
        "eslint": "^6.7.2",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.3",
        "javascript-obfuscator": "^1.7.0",
        "jest": "^26.0.1",
        "jsdoc": "^3.6.4",
        "node": "^14.4.0",
        "npm-dts": "^1.3.1",
        "prettier": "^2.0.5",
        "ts-jest": "^26.1.0",
        "typedoc": "^0.17.7",
        "typescript": "~3.9.3"
    }
}
